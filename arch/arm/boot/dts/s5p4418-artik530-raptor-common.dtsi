	nx-devfreq {
		pll = <3>;
		supply_name = "vdd_arm_regulator";
		vdd_arm_regulator-supply = <&VCC_DCDC1>;
		status = "okay";
	};

};

&pwm {
	pinctrl-names = "default";
	pinctrl-0 = <&pwm0_pin &pwm2_pin>;
	samsung,pwm-outputs = <0>, <2>;
	status = "okay";
};

&serial1 {
	status = "okay";
	pinctrl-0 = <&serial1_pin>, <&serial1_flow_cts>, <&serial1_flow_rts>;
};

&serial4 {
	status = "okay";
};

&spi_2 {
	status = "okay";
	cs-gpios = <&gpio_c 10 0>;
	spidev@0 {
		compatible = "linux,spidev";
		reg = <0>;
		spi-max-frequency = <10000000>;
		controller-data {
			samsung,spi-feedback-delay = <0>;
		};
	};
};

&i2c_2 {
	#address-cells = <1>;
	#size-cells = <0>;
	rate = <400000>;
	status = "okay";

	/* touch screen */
	goodix@5d {
		compatible = "goodix,gt911";
		reg = <0x5d>;
		interrupt-parent = <&gpio_d>;
		interrupts = <8 IRQ_TYPE_EDGE_FALLING>;
		irq-gpios = <&gpio_d 8 0>;
		reset-gpios = <&gpio_b 0 0>;
		touchscreen-inverted-x;
//		touchscreen-inverted-y;
//		touchscreen-swapped-x-y;
		status = "disabled";
	};
};

&dp_drm {
	status = "okay";
	ports {
		port@0 {
			reg = <0>;
			back_color = < 0x0 >;
			color_key = < 0x0 >;
			plane-names = "rgb", "primary", "video";
		};
		port@1 {
			reg = <1>;
			back_color = < 0x0 >;
			color_key = < 0x0 >;
			plane-names = "primary", "video";
		};
	};
};

&dp_drm_mipi {
	hs_bitrate = <420>;
	lp_bitrate = <100>;
	skip_panel_check = <1>;
	status = "disabled";

	display-timing {
		clock-frequency = <81244800>;
		hactive = <800>;
		vactive = <1280>;
		hfront-porch = <110>;
		hback-porch = <130>;
		hsync-len = <8>;
		vfront-porch = <12>;
		vback-porch = <7>;
		vsync-len = <3>;
	};

	panel@0 {
		compatible = "hx8394d";
		reg = <0>;
		vdd3-supply = <&VCC_LDO0>;
		vci-supply = <&VCC_LDO8>;
		reset-gpio = <&gpio_c 11 0>;
		power-on-delay= <50>;
		reset-delay = <100>;
		init-delay = <100>;
	};

	dp_control {
		clk_src_lv0 = <0>;
		clk_div_lv0 = <12>;
		clk_src_lv1 = <7>;
		clk_div_lv1 = <1>;
		out_format = <3>;
		vs_start_offset = <0>;
		ev_start_offset = <0>;
		vs_end_offset = <0>;
		ev_end_offset = <0>;
	};
};

&dp_drm_hdmi {
	ddc-i2c-bus = <&i2c_gpio7>;
	status = "okay";
};

&dp_drm_lvds {
	status = "disabled";

	remote-endpoint = <&lvds_panel>;

	dp_control {
		clk_src_lv0 = <0>;
		clk_div_lv0 = <16>;
		clk_src_lv1 = <7>;
		clk_div_lv1 = <1>;
		out_format = <3>;
	};
};

&dw_mmc_1 {
	pinctrl-names = "default";
	mmc-pwrseq = <&wifi_pwrseq>;

	cap-sd-highspeed;
	cap-sdio-irq;
	mmc-ddr-3_3v;
	broken-cd;
	non-removable;
	disable-wp;

	nexell,drive_dly = <0x0>;
	nexell,drive_shift = <0x03>;
	nexell,sample_dly = <0x00>;
	nexell,sample_shift = <0x02>;

	clock-frequency = <400000000>;
	status = "okay";

	brcmf: wifi@1 {
		reg = <1>;
		compatible = "brcm,bcm4329-fmac";
		interrupt-parent = <&gpio_b>;
		interrupts = <11 IRQ_TYPE_LEVEL_HIGH>; 
		interrupt-names = "host-wake";
		brcm,drive-strength = <6>;
	};
};

&dw_mmc_2 {
	num-slots = <1>;
	bus-width = <4>;
	cap-sd-highspeed;
	clock-frequency = <400000000>;
	card-detect-delay = <200>;
	disable-wp;
	cd-inverted;
	nexell,drive_dly = <0x0>;
	nexell,drive_shift = <0x02>;
	nexell,sample_dly = <0x00>;
	cd-gpios = <&gpio_c 17 0>;
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc2_cclk &sdmmc2_cmd &sdmmc2_bus4>;
};
